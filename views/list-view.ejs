                                <button class="tg-btn tg-btn-lg" ng-click="send()">LOGIN now</button>
                            </fieldset>
                        </form>
                    </div>
                    <div role="tabpanel" class="tab-pane tg-haslayout" id="tg-signup-formarea" ng-controller="signupController">
                        <form class="tg-form-modal tg-form-signup">
                            <span style="color:red" ng-cloak>{{error}}</span>
                            <fieldset>
                                <div class="form-group">
                                    <span class="select">
                                        <label>Select who you are?</label>                                
                                        <select id="select-type"  ng-model="user.typeOfUser">                                            
                                            <option>Doctor</option>
                                            <option>Patient</option>
                                            <option>Hospital</option>
                                            <option>Clinic</option>
                                            <option>Pharmacy</option>
                                            <option>Radiology Center</option>
                                            <option>Laboratory Center</option>
                                            <option>Fitness Center</option>
                                        </select>
                                    </span>
                                </div>
                                <div class="form-group" id="specialization">
                                    <span class="select">
                                        <label>Select your specialization</label> 
                                        <select  ng-model="user.specializtion">                                            
                                            <option>Eye specialist</option>
                                            <option>Specialty two</option>
                                            <option>Specialty three</option>
                                        </select>
                                    </span>
                                </div>
                                <div class="form-group" id="category-of-type">
                                    <span class="select">
                                        <label>What category? </label>                                        
                                        <select  ng-model="category_of_type">                                            
                                            <option>Tertiary</option>
                                            <option>Secondary</option>
                                            <option>Primary</option>
                                            <option>Private</option>                                           
                                        </select>
                                    </span>
                                </div>                                
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" placeholder="First Name*" id="firstname" ng-model="user.firstname">
                                </div>                                
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" placeholder="lastname*" id="lastname" ng-model="user.lastname">
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" placeholder="Name*" id="name" ng-model="user.name">
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" placeholder="Address*" ng-model="user.address">
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" placeholder="City/Town*" id="firstname" ng-model="user.city">
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" placeholder="Phone Number*" id="phone" ng-model="user.phone">
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" placeholder="website url" id="website" ng-model="user.website">
                                </div>
                                <div class="form-group">
                                    <input type="email" class="form-control placeholder-color" placeholder="Email*" id="firstname" ng-model="user.email">
                                </div>
                                <div class="form-group">
                                    <input type="password" class="form-control" placeholder="Password" ng-model="user.password">
                                </div>                                                   
                                <div class="form-group">
                                    <input type="text" class="form-control placeholder-color" name="zip_code" placeholder="ZIP Code" ng-model="user.zip_code">
                                </div>
                                <div class="form-group tg-checkbox">
                                    <label>
                                        <input type="checkbox" class="form-control" ng-model="user.check">
                                        I agree with the terms and conditions
                                    </label>
                                </div>                                
                                <button class="tg-btn tg-btn-lg" ng-click="submit()">Create an Account</button>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <script type="text/javascript">
        window.addEventListener('load',function(){
                var type = document.getElementById("select-type");
                var specialization = document.getElementById("specialization");
                var name = document.getElementById("name");
                var firstname = document.getElementById("firstname");
                var lastname = document.getElementById("lastname");
                var phone = document.getElementById("phone");
                var category_of_type = document.getElementById("category-of-type");               
                var website = document.getElementById("website");                
                initForm(); 
                function setType() {                    
                    switch(type.value) {
                        case "Doctor":
                            specialization.style.display = "block";
                            firstname.style.display = "block";
                            lastname.style.display = "block";
                            phone.style.display = "block";
                            name.style.display = "none";
                            website.style.display = "none";
                            category_of_type.style.display = "none";                                                      
                            break;
                        case "Patient":                                             
                            firstname.style.display = "block";
                            lastname.style.display = "block";
                            phone.style.display = "block";
                            specialization.style.display = "none";
                            website.style.display = "none";
                            name.style.display = "none";
                            category_of_type.style.display = "none";             
                            break;
                        case "Hospital":                                                    
                            category_of_type.style.display = "block";                            
                            website.style.display = "block";
                            name.style.display = "block";
                            specialization.style.display = "none";
                            firstname.style.display = "none";
                            lastname.style.display = "none";
                            break;
                        case "Clinic":                                                       
                            category_of_type.style.display = "block";
                            website.style.display = "block";
                            name.style.display = "block";
                            specialization.style.display = "none";
                            firstname.style.display = "none";
                            lastname.style.display = "none";                            
                            break;
                        case "Radiology Center":
                            name.style.display = "block";
                            specialization.style.display = "none"; 
                            category_of_type.style.display = "none";
                            firstname.style.display = "none";
                            lastname.style.display = "none";                                                     
                            break;
                        case "Laboratory Center":
                            name.style.display = "block";
                            specialization.style.display = "none"; 
                            category_of_type.style.display = "none";
                            firstname.style.display = "none";
                            lastname.style.display = "none";                                                       
                            break;
                        case "Pharmacy":
                            name.style.display = "block";
                            specialization.style.display = "none"; 
                            category_of_type.style.display = "none";
                            firstname.style.display = "none";
                            lastname.style.display = "none";                                                       
                            break;
                        case "Fitness Center":
                            name.style.display = "block";
                            specialization.style.display = "none";
                            category_of_type.style.display = "none";
                            firstname.style.display = "none";
                            lastname.style.display = "none";                                                     
                            break;
                        default:
                            alert("You did not identify who you are!");
                            break;
                    }
                }

                function initForm() {
                    category_of_type.style.display = "none";
                    name.style.display = "none";
                    firstname.style.display = "none";
                    lastname.style.display = "none";
                    phone.style.display = "none";
                    website.style.display = "none";
                    specialization.style.display = "none";
                }
                type.addEventListener('change',setType,false)
            }, false)
        </script>
