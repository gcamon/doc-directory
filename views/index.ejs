        <script type="text/javascript">
        window.addEventListener('load',function(){
                var type = document.getElementById("select-type");
                var specialization = document.getElementById("specialization");
                var name = document.getElementById("name");
                var firstname = document.getElementById("firstname");
                var lastname = document.getElementById("lastname");
                var phone = document.getElementById("phone");
                var category_of_type = document.getElementById("category-of-type");               
                var website = document.getElementById("website");                
                category_of_type.style.display = "none";
                name.style.display = "none";
                firstname.style.display = "none";
                lastname.style.display = "none";
                phone.style.display = "none";
                website.style.display = "none";
                specialization.style.display = "none";
                function setType() {                    
                    switch(type.value) {
                        case "Doctor":
                            specialization.style.display = "block";
                            firstname.style.display = "block";
                            lastname.style.display = "block";
                            phone.style.display = "block";
                            name.style.display = "none";
                            website.style.display = "none";
                            category_of_type.style.display = "none";                                                      
                            break;
                        case "Patient":                                             
                            firstname.style.display = "block";
                            lastname.style.display = "block";
                            phone.style.display = "block";
                            specialization.style.display = "none";
                            website.style.display = "none";
                            name.style.display = "none";
                            break;
                        case "Hospital":                                                    
                            category_of_type.style.display = "block";                            
                            website.style.display = "block";
                            name.style.display = "block";
                            specialization.style.display = "none";
                            break;
                        case "Clinic":                                                       
                            category_of_type.style.display = "block";
                            website.style.display = "block";
                            name.style.display = "block";
                            specialization.style.display = "none";                            
                            break;
                        case "Radiology Center":
                            name.style.display = "block";
                            specialization.style.display = "none"; 
                            category_of_type.style.display = "none";                                                     
                            break;
                        case "Laboratory Center":
                            name.style.display = "block";
                            specialization.style.display = "none"; 
                            category_of_type.style.display = "none";                                                       
                            break;
                        case "Fitness Center":
                            name.style.display = "block";
                            specialization.style.display = "none";
                            category_of_type.style.display = "none";                                                     
                            break;
                        default:
                            alert("You did not identify who you are!");
                            break;
                    }
                }

                type.addEventListener('change',setType,false)
            }, false)
        </script>
